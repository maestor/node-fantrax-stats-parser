{
  "functions": {
    "api/index.ts": {
      "includeFiles": "csv/**"
    },
    "api/[...path].ts": {
      "includeFiles": "csv/**"
    }
  },
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "^/api$",
      "dest": "/api/index"
    },
    {
      "src": "^/api/(.*)$",
      "dest": "/api/[...path]?path=$1"
    },
    {
      "src": "^/$",
      "dest": "/api/index"
    },
    {
      "src": "^/(.*)$",
      "dest": "/api/[...path]?path=$1"
    }
  ]
}